<!DOCTYPE Map>
<Map buffer-size="100" buffer-size-collisions="200">

    <Style name="dot">
        <Rule>
            <DotSymbolizer />
        </Rule>
    </Style>

    <Style name="line">
        <Rule>
            <LineSymbolizer stroke="black"/>
        </Rule>
    </Style>

    <Style name="line-pattern">
        <Rule>
            <LinePatternSymbolizer file="../images/dummy.png" />
        </Rule>
    </Style>

    <Style name="polygon">
        <Rule>
            <PolygonSymbolizer fill="black"/>
        </Rule>
    </Style>

    <Style name="polygon-pattern">
        <Rule>
            <PolygonPatternSymbolizer file="../images/dummy.png" />
        </Rule>
    </Style>

    <Style name="point">
        <Rule>
            <PointSymbolizer />
        </Rule>
    </Style>

    <Style name="marker1">
        <Rule>
            <MarkersSymbolizer />
        </Rule>
    </Style>

    <Style name="marker2">
        <Rule>
            <MarkersSymbolizer ignore-placement="true" allow-overlap="true" />
        </Rule>
    </Style>

    <Style name="marker3">
        <Rule>
            <MarkersSymbolizer ignore-placement="true" />
        </Rule>
    </Style>

    <Style name="marker4">
        <Rule>
            <MarkersSymbolizer allow-overlap="true" />
        </Rule>
    </Style>

    <Style name="text1">
        <Rule>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                size="16"
                placement="point"
                allow-overlap="true">"Text"</TextSymbolizer>
        </Rule>
    </Style>

    <Style name="text2">
        <Rule>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                size="16"
                placement="point">"Text"</TextSymbolizer>
        </Rule>
    </Style>

    <Style name="shield1">
        <Rule>
            <ShieldSymbolizer
                face-name="DejaVu Sans Book"
                file="../images/dummy.png"
                size="16"
                placement="point"
                allow-overlap="true">"Text"</ShieldSymbolizer>
        </Rule>
    </Style>

    <Style name="shield2">
        <Rule>
            <ShieldSymbolizer
                face-name="DejaVu Sans Book"
                file="../images/dummy.png"
                size="16"
                placement="point">"Text"</ShieldSymbolizer>
        </Rule>
    </Style>

    <Style name="raster">
        <Rule>
            <RasterSymbolizer />
        </Rule>
    </Style>

    <Style name="building">
        <Rule>
            <BuildingSymbolizer />
        </Rule>
    </Style>

    <Style name="group">
        <Rule>
            <GroupSymbolizer
                start-column="1"
                num-columns="2"
                placement="point"
                minimum-distance="10"
                spacing="20">
                <PairLayout item-margin="1"/>
                <GroupRule>
                    <PointSymbolizer />
                    <TextSymbolizer
                        face-name="DejaVu Sans Book"
                        size="16"
                        placement="point"
                        dy="10">"Text"</TextSymbolizer>
                </GroupRule>
            </GroupSymbolizer>
        </Rule>
    </Style>

    <Style name="collision">
        <Rule>
            <CollisionSymbolizer />
        </Rule>
    </Style>

    <Style name="debug">
        <Rule>
            <DebugSymbolizer />
        </Rule>
    </Style>

    <Style name="mix">
        <Rule>
            <DotSymbolizer />
            <LineSymbolizer stroke="black"/>
            <LinePatternSymbolizer file="../images/dummy.png" />
            <PolygonSymbolizer fill="black"/>
            <PolygonPatternSymbolizer file="../images/dummy.png" />
            <PointSymbolizer />
            <MarkersSymbolizer />
            <MarkersSymbolizer ignore-placement="true" allow-overlap="true" />
            <MarkersSymbolizer ignore-placement="true" />
            <MarkersSymbolizer allow-overlap="true" />
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                size="16"
                placement="point"
                allow-overlap="true">"Text"</TextSymbolizer>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                size="16"
                placement="point">"Text"</TextSymbolizer>
            <ShieldSymbolizer
                face-name="DejaVu Sans Book"
                file="../images/dummy.png"
                size="16"
                placement="point"
                allow-overlap="true">"Text"</ShieldSymbolizer>
            <ShieldSymbolizer
                face-name="DejaVu Sans Book"
                file="../images/dummy.png"
                size="16"
                placement="point">"Text"</ShieldSymbolizer>
            <RasterSymbolizer />
            <BuildingSymbolizer />
            <GroupSymbolizer
                start-column="1"
                num-columns="2"
                placement="point"
                minimum-distance="10"
                spacing="20">
                <PairLayout item-margin="1"/>
                <GroupRule>
                    <PointSymbolizer />
                    <TextSymbolizer
                        face-name="DejaVu Sans Book"
                        size="16"
                        placement="point"
                        dy="10">"Text"</TextSymbolizer>
                </GroupRule>
            </GroupSymbolizer>
            <CollisionSymbolizer />
            <DebugSymbolizer />
        </Rule>
    </Style>

    <Layer name="dot">
        <StyleName>dot</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="inline">
                x, y
                0, 0
            </Parameter>
        </Datasource>
    </Layer>

    <Layer name="line">
